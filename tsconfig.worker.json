{
    "compilerOptions": {
        "target": "ES2021",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "lib": [
            "ES2021"
        ],
        "types": [
            "@cloudflare/workers-types"
        ],
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true
    },
    "include": [
        "src/worker/**/*.ts"
    ]
}